{
  "cli": {
    "version": ">=16.17.4"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {}
    },
    "production": {
      "ios": {
        "cache": {
          "disabled": true
        }
      },
      "distribution": "store",
      "android": {
