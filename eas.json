      }
    },
    "testflight": {
      "ios": {
        "distribution": "store",
        "autoIncrement": "buildNumber"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "6748543318",
        "ascAppId": "lionmartinez@gmail.com"
      },
      "android": {}
    }
  }
}
